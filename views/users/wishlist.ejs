<script>
     function addToCart(proId,price) {
   
   console.log(proId)
$.ajax({
   url: '/addToCart',
   data: {
     productId: proId,
     price: price
   },
   method: "post",
   success: (response) => {
     if (response.success) {
       $('#table-shopping-cart').load('/wishlist #table-shopping-cart')
       console.log(response);
       Swal.fire({
         title: 'Added',
         text: 'Item added to Cart',
         icon: 'success',
         timer: 1000,
         showConfirmButton: false
       })
     } else {
       Swal.fire({
         text: 'Item already in Cart',
         icon: 'info',
         timer: 1000,
         showConfirmButton: false
       })
     }
     console.log(response);
   }
 })
}

function deletewish(proid) {

        

Swal.fire({
    title: 'Are you sure?',
    text: "You wan't to delete!",
    icon: 'warning',
    showCancelButton: true,
    confirmButtonColor: '#3085d6',
    cancelButtonColor: '#d33',
    confirmButtonText: 'yes, delete!'
}).then((result) => {
    if (result.isConfirmed) {
        console.log(proid);
        $.ajax({
            url: "/deletewish",
            data: {
                productId: proid
            },
            method: 'post',
            success: (response) => {
                console.log("warning")

                $('#table-shopping-cart').load('/wishlist #table-shopping-cart')
                console.log(response);
                Swal.fire({
                    title: 'deleted!',
                    text: 'Your deleted',
                    icon: 'success',
                    showConfirmButton: false
                })
                       

                    }

                })
            }
        })
    }


</script>


